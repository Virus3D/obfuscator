{
    "name": "virus3d/php-obfuscator",
    "description": "PHP obfuscator",
    "type": "project",
    "keywords": [
        "php",
        "obfuscator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Virus3D",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-fileinfo": "*",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^4.17.1",
        "symfony/config": "^6.3",
        "symfony/yaml": "^6.3",
        "symfony/finder": "^6.3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.43",
        "friendsofphp/php-cs-fixer": "^3.59",
        "nunomaduro/phpinsights": "^2.11",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-symfony": "^1.4",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PhpObfuscator\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
